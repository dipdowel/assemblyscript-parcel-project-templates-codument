  "scripts": {
    "clean:dist": "rimraf dist/*",
    "clean:build": "rimraf build/*",
    "start": "yarn clean:dist && parcel src/index.html",
    "build": "yarn clean:dist && parcel build src/index.html",
    "build-subdir": "yarn clean:dist && parcel build --public-url . src/index.html"
  },
  "devDependencies": {
    "parcel": "^2.9.3",
    "parcel-config-assemblyscript-codument": "0.1.11",
    "parcel-transformer-assemblyscript-codument": "^0.1.8",
    "rimraf": "^5.0.1",
    "prettier": "^3.0.3",
    "cosmiconfig": "7.1.0"
  },
  "resolutions": {
    "cosmiconfig": "7.1.0"
  },
  "overrides": {
    "cosmiconfig": "7.1.0"
  }
}
