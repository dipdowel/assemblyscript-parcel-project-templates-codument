    "parcel": "^2.9.3",
    "parcel-config-assemblyscript-codument": "0.3.0",
    "parcel-transformer-assemblyscript-codument": "^0.3.0",
    "rimraf": "^5.0.1",
    "prettier": "^3.0.3",
    "cosmiconfig": "8.3.6"
  },
  "resolutions": {
    "cosmiconfig": "8.3.6"
  },
  "overrides": {
    "cosmiconfig": "8.3.6"
  }
}
